<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/RelativeLayout_SearchArtists"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="29dp"
        android:id="@+id/SearchArtists_index" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/sym_def_app_icon"
        android:layout_marginLeft="31dp"
        android:layout_marginStart="31dp"
        android:id="@+id/SearchArtists_imageView"
        android:layout_marginTop="13dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/SearchArtists_index"
        android:layout_toEndOf="@+id/SearchArtists_index" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/SearchArtists_index"
        android:layout_toRightOf="@+id/SearchArtists_imageView"
        android:layout_toEndOf="@+id/SearchArtists_imageView"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:id="@+id/SearchArtists_name" />
</RelativeLayout>