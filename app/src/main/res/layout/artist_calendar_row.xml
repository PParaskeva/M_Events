<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="17dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="20dp"
        android:id="@+id/ArtistCalendar_index" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/ArtistCalendar_index"
        android:layout_toRightOf="@+id/ArtistCalendar_index"
        android:layout_toEndOf="@+id/ArtistCalendar_index"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:id="@+id/artistCalendar_eventName" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:id="@+id/artistCalendar_venueName"
        android:layout_below="@+id/artistCalendar_eventName"
        android:layout_alignLeft="@+id/artistCalendar_eventName"
        android:layout_alignStart="@+id/artistCalendar_eventName" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/artistCalendar_venueName"
        android:layout_alignRight="@+id/artistCalendar_venueName"
        android:layout_alignEnd="@+id/artistCalendar_venueName"
        android:layout_marginTop="16dp"
        android:id="@+id/artistCalendar_cityName" />
    </RelativeLayout>

</android.support.v7.widget.CardView>